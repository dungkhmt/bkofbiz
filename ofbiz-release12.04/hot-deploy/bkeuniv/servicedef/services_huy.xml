<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BkEuniv Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service> 
    
    <service name="getProjectDeclaration" engine="java" 
    		location="src.org.ofbiz.bkeuniv.projectdeclaration.ProjectDeclaration" invoke="getProjectDeclarationOfStaff">
    	<attribute name="projectDeclarations" mode="OUT" type="List"></attribute>
    </service>
    
    <service name="getProjectCategory" engine="java" 
    		location="src.org.ofbiz.bkeuniv.projectdeclaration.ProjectDeclaration" invoke="getProjectCategory">
    	<attribute name="projectCategorys" mode="OUT" type="List"></attribute>
    </service>
    
    <service name="getProjectStatus" engine="java" 
    		location="src.org.ofbiz.bkeuniv.projectdeclaration.ProjectDeclaration" invoke="getProjectStatus">
    	<attribute name="projectStatuss" mode="OUT" type="List"></attribute>
    </service>
    
    <service name="getProjectParticipationRole" engine="java" 
    		location="src.org.ofbiz.bkeuniv.projectdeclaration.ProjectDeclaration" invoke="getProjectParticipationRole">
    	<attribute name="projectParticipationRoles" mode="OUT" type="List"></attribute>
    </service>
    
    <service name="createProjectDeclaration" engine="java" 
    		location="src.org.ofbiz.bkeuniv.projectdeclaration.ProjectDeclaration" invoke="createProjectDeclaration">
    	<attribute name="projectCategoryId[]" mode="IN" type="List"></attribute>
    	<attribute name="projectName" mode="IN" type="String"></attribute>
    	<attribute name="startDate" mode="IN" type="String"></attribute>
    	<attribute name="endDate" mode="IN" type="String"></attribute>
    	<attribute name="projectStatusId[]" mode="IN" type="List"></attribute>
    	<attribute name="researchProgram" mode="IN" type="String"></attribute>
    	<attribute name="projectParticipationRoleId[]" mode="IN" type="List"></attribute>
    	<attribute name="approverStaffId[]" mode="IN" type="List"></attribute>
    	
    	<attribute name="projectDeclarations" mode="OUT" type="Map"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="updateProjectDeclaration" engine="java" 
    		location="src.org.ofbiz.bkeuniv.projectdeclaration.ProjectDeclaration" invoke="updateProjectDeclaration">
    	<attribute name="projectDeclarationId" mode="IN" type="String"></attribute>
    	<attribute name="projectCategoryId[]" mode="IN" type="List"></attribute>
    	<attribute name="projectName" mode="IN" type="String"></attribute>
    	<attribute name="startDate" mode="IN" type="String"></attribute>
    	<attribute name="endDate" mode="IN" type="String"></attribute>
    	<attribute name="projectStatusId[]" mode="IN" type="List"></attribute>
    	<attribute name="researchProgram" mode="IN" type="String"></attribute>
    	<attribute name="projectParticipationRoleId[]" mode="IN" type="List"></attribute>
    	<attribute name="approverStaffId" mode="IN" type="String"></attribute>
    	
    	<attribute name="projectDeclarations" mode="OUT" type="Map"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="deleteProjectDeclaration" engine="java" 
    		location="src.org.ofbiz.bkeuniv.projectdeclaration.ProjectDeclaration" invoke="deleteProjectDeclaration">
    	<attribute name="projectDeclarationId" mode="IN" type="String"></attribute>
    	<attribute name="result" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="getWorkProgress" engine="java" 
    		location="src.org.ofbiz.bkeuniv.workprogress.WorkProgress" invoke="getWorkProgress">
    	<attribute name="workProgress" mode="OUT" type="List"></attribute>
    </service>
    
    <service name="createWorkProgress" engine="java" 
    		location="src.org.ofbiz.bkeuniv.workprogress.WorkProgress" invoke="createWorkProgress">
    	<attribute name="period" mode="IN" type="String"></attribute>
    	<attribute name="position" mode="IN" type="String"></attribute>
    	<attribute name="specialization" mode="IN" type="String"></attribute>
    	<attribute name="institution" mode="IN" type="String"></attribute>
    	
    	<attribute name="workProgress" mode="OUT" type="org.ofbiz.entity.GenericValue"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="updateWorkProgress" engine="java" 
    		location="src.org.ofbiz.bkeuniv.workprogress.WorkProgress" invoke="updateWorkProgress">
    	<attribute name="workProgressId" mode="IN" type="String"></attribute>
    	<attribute name="period" mode="IN" type="String"></attribute>
    	<attribute name="position" mode="IN" type="String"></attribute>
    	<attribute name="specialization" mode="IN" type="String"></attribute>
    	<attribute name="institution" mode="IN" type="String"></attribute>
    	
    	<attribute name="workProgress" mode="OUT" type="org.ofbiz.entity.GenericValue"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="deleteWorkProgress" engine="java" 
    		location="src.org.ofbiz.bkeuniv.workprogress.WorkProgress" invoke="deleteWorkProgress">
    	<attribute name="workProgressId" mode="IN" type="String"></attribute>
    	<attribute name="result" mode="OUT" type="String"></attribute>
    </service>
    
</services>