<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BkEuniv Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service> 
    
    <service name="getResearchDomain" engine="java" 
    		location="src.org.ofbiz.bkeuniv.researchdomain.ResearchDomain" invoke="getResearchDomain">
    	<attribute name="researchDomainId" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="researchDomainName" mode="IN" type="String" optional="true"></attribute>   	
    	<attribute name="researchDomain" mode="OUT" type="List"></attribute>
    	<attribute name="result" mode="OUT" type="List"></attribute>
    	<attribute name="count" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="createResearchDomain" engine="java" 
    		location="src.org.ofbiz.bkeuniv.researchdomain.ResearchDomain" invoke="createResearchDomain">
    	<attribute name="researchDomainId" mode="IN" type="String"></attribute>   
    	<attribute name="researchDomainName" mode="IN" type="String"></attribute>   	
    	<attribute name="researchDomain" mode="OUT" type="org.ofbiz.entity.GenericValue"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="updateResearchDomain" engine="java" 
    		location="src.org.ofbiz.bkeuniv.researchdomain.ResearchDomain" invoke="updateResearchDomain">
    	<attribute name="researchDomainId" mode="IN" type="String"></attribute>
    	<attribute name="researchDomainName" mode="IN" type="String"></attribute>   	
    	
    	<attribute name="researchDomain" mode="OUT" type="Map"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    
      <service name="deleteResearchDomain" engine="java" 
    		location="src.org.ofbiz.bkeuniv.researchdomain.ResearchDomain" invoke="deleteResearchDomain">
    	<attribute name="researchDomainId" mode="IN" type="String"></attribute>
    	
    	<attribute name="result" mode="OUT" type="String"></attribute>
	</service>
	
	
	<service name="getResearchSpeciality" engine="java" 
    		location="src.org.ofbiz.bkeuniv.researchspeciality.ResearchSpeciality" invoke="getResearchSpeciality">
    	<attribute name="researchSpecialityId" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="researchSpecialityName" mode="IN" type="String" optional="true"></attribute>   
    	<attribute name="researchDomainId" mode="IN" type="String" optional="true"></attribute>
    		
    	<attribute name="researchSpeciality" mode="OUT" type="List"></attribute>
    	<attribute name="result" mode="OUT" type="List"></attribute>
    	<attribute name="count" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="createResearchSpeciality" engine="java" 
    		location="src.org.ofbiz.bkeuniv.researchspeciality.ResearchSpeciality" invoke="createResearchSpeciality">
    	<attribute name="researchSpecialityId" mode="IN" type="String"></attribute>   
    	<attribute name="researchSpecialityName" mode="IN" type="String"></attribute>  
    	<attribute name="researchDomainId[]" mode="IN" type="List"></attribute>  
    	 	
    	<attribute name="researchSpeciality" mode="OUT" type="Map"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="updateResearchSpeciality" engine="java" 
    		location="src.org.ofbiz.bkeuniv.researchspeciality.ResearchSpeciality" invoke="updateResearchSpeciality">
    	<attribute name="researchSpecialityId" mode="IN" type="String"></attribute>
    	<attribute name="researchSpecialityName" mode="IN" type="String"></attribute>  
    	<attribute name="researchDomainId[]" mode="IN" type="List"></attribute> 	
    	
    	<attribute name="researchSpeciality" mode="OUT" type="Map"></attribute>
    	
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    
      <service name="deleteResearchSpeciality" engine="java" 
    		location="src.org.ofbiz.bkeuniv.researchspeciality.ResearchSpeciality" invoke="deleteResearchSpeciality">
    	<attribute name="researchSpecialityId" mode="IN" type="String"></attribute>
    	
    	<attribute name="result" mode="OUT" type="String"></attribute>
	</service>
	
	
	<service name="getStaffResearchSpeciality" engine="java" 
    		location="src.org.ofbiz.bkeuniv.staffresearchspeciality.StaffResearchSpeciality" invoke="getStaffResearchSpeciality">
    	<attribute name="staffResearchSpecialityId" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="staffId" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="researchSpecialityId" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="fromDate" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="thruDate" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="staffResearchSpeciality" mode="OUT" type="List"></attribute>
    </service>
    
    <service name="createStaffResearchSpeciality" engine="java" 
    		location="src.org.ofbiz.bkeuniv.staffresearchspeciality.StaffResearchSpeciality" invoke="createStaffResearchSpeciality">
    	
    	<attribute name="researchSpecialityId[]" mode="IN" type="List"></attribute>
    	<attribute name="fromDate" mode="IN" type="String"></attribute>
    	<attribute name="thruDate" mode="IN" type="String"></attribute>
    	<attribute name="staffId[]" mode="IN" type="List"></attribute>
    	
    	<attribute name="staffResearchSpeciality" mode="OUT" type="Map"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="updateStaffResearchSpeciality" engine="java" 
    		location="src.org.ofbiz.bkeuniv.staffresearchspeciality.StaffResearchSpeciality" invoke="updateStaffResearchSpeciality">
    	<attribute name="staffResearchSpecialityId" mode="IN" type="String"></attribute>
    	<attribute name="staffId[]" mode="IN" type="List"></attribute>
    	<attribute name="researchSpecialityId[]" mode="IN" type="List"></attribute>
    	<attribute name="fromDate" mode="IN" type="String"></attribute>
    	<attribute name="thruDate" mode="IN" type="String"></attribute>
    	
    	<attribute name="staffResearchSpeciality" mode="OUT" type="Map"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="deleteStaffResearchSpeciality" engine="java" 
    		location="src.org.ofbiz.bkeuniv.staffresearchspeciality.StaffResearchSpeciality" invoke="deleteStaffResearchSpeciality">
    	<attribute name="staffResearchSpecialityId" mode="IN" type="String"></attribute>
    	
    	<attribute name="result" mode="OUT" type="String"></attribute>
    </service>
    
    
</services>