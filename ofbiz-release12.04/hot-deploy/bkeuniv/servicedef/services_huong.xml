<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BkEuniv Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service> 
    
    
    <service name="getForeignLanguageCatalog" engine="java" 
    		location="src.org.ofbiz.bkeuniv.foreignlanguagecatalog.ForeignLanguageCatalog" invoke="getForeignLanguageCatalog">
    	<attribute name="foreignLanguageCatalogId" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="foreignLanguageCatalogName" mode="IN" type="String" optional="true"></attribute>   
    		
    	<attribute name="foreignLanguageCatalog" mode="OUT" type="List"></attribute>
    	<attribute name="result" mode="OUT" type="List"></attribute>
    	<attribute name="count" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="createForeignLanguageCatalog" engine="java" 
    		location="src.org.ofbiz.bkeuniv.foreignlanguagecatalog.ForeignLanguageCatalog" invoke="createForeignLanguageCatalog">
    	<attribute name="foreignLanguageCatalogId" mode="IN" type="String"></attribute>   
    	<attribute name="foreignLanguageCatalogName" mode="IN" type="String"></attribute>  
    	<attribute name="foreignLanguageCatalog" mode="OUT" type="Map"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="updateForeignLanguageCatalog" engine="java" 
    		location="src.org.ofbiz.bkeuniv.foreignlanguagecatalog.ForeignLanguageCatalog" invoke="updateForeignLanguageCatalog">
    	<attribute name="foreignLanguageCatalogId" mode="IN" type="String"></attribute>
    	<attribute name="foreignLanguageCatalogName" mode="IN" type="String"></attribute>  
    	
    	<attribute name="foreignLanguageCatalog" mode="OUT" type="Map"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    
      <service name="deleteForeignLanguageCatalog" engine="java" 
    		location="src.org.ofbiz.bkeuniv.foreignlanguagecatalog.ForeignLanguageCatalog" invoke="deleteForeignLanguageCatalog">
    	<attribute name="foreignLanguageCatalogId" mode="IN" type="String"></attribute>
    	
    	<attribute name="result" mode="OUT" type="String"></attribute>
	</service>
	
	
	<service name="getForeignLanguage" engine="java" 
    		location="src.org.ofbiz.bkeuniv.foreignlanguage.ForeignLanguage" invoke="getForeignLanguage">
    	<attribute name="foreignLanguageId" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="staffId" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="foreignLanguageCatalogId" mode="IN" type="String" optional="true"></attribute>   
    	<attribute name="listen" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="speaking" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="reading" mode="IN" type="String" optional="true"></attribute>
    	<attribute name="writing" mode="IN" type="String" optional="true"></attribute>
    	
    		
    	<attribute name="foreignLanguage" mode="OUT" type="List"></attribute>
    	<attribute name="result" mode="OUT" type="List"></attribute>
    	<attribute name="count" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="createForeignLanguage" engine="java" 
    		location="src.org.ofbiz.bkeuniv.foreignlanguage.ForeignLanguage" invoke="createForeignLanguage">
    	<attribute name="foreignLanguageId" mode="IN" type="String"></attribute> 
    	<attribute name="staffId[]" mode="IN" type="List"></attribute>   
    	<attribute name="foreignLanguageCatalogId[]" mode="IN" type="List"></attribute>  
    	<attribute name="listen" mode="IN" type="String"></attribute>  
    	<attribute name="speaking" mode="IN" type="String"></attribute> 
    	<attribute name="reading" mode="IN" type="String"></attribute> 
    	<attribute name="writing" mode="IN" type="String"></attribute> 
    	 	
    	<attribute name="foreignLanguage" mode="OUT" type="Map"></attribute>
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    <service name="updateForeignLanguage" engine="java" 
    		location="src.org.ofbiz.bkeuniv.foreignlanguage.ForeignLanguage" invoke="updateForeignLanguage">
    	<attribute name="foreignLanguageId" mode="IN" type="String"></attribute>
    	<attribute name="staffId[]" mode="IN" type="List"></attribute> 
    	<attribute name="foreignLanguageCatalogId[]" mode="IN" type="List"></attribute> 			
    	<attribute name="listen" mode="IN" type="String"></attribute>  
    	<attribute name="speaking" mode="IN" type="String"></attribute>  
    	<attribute name="reading" mode="IN" type="String"></attribute>  
    	<attribute name="writing" mode="IN" type="String"></attribute>  
    	
    	
    	<attribute name="foreignLanguage" mode="OUT" type="Map"></attribute>
    	
    	<attribute name="message" mode="OUT" type="String"></attribute>
    </service>
    
    
      <service name="deleteForeignLanguage" engine="java" 
    		location="src.org.ofbiz.bkeuniv.foreignlanguage.ForeignLanguage" invoke="deleteForeignLanguage">
    	<attribute name="foreignLanguageId" mode="IN" type="String"></attribute>
    	
    	<attribute name="result" mode="OUT" type="String"></attribute>
	</service>
    
    
</services>